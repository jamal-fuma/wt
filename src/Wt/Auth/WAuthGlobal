#ifndef WAUTHGLOBAL_H_
#define WAUTHGLOBAL_H_

#include <Wt/WGlobal>

namespace Wt {
  namespace Auth {

enum ClientSecretMethod
{
  HttpAuthorizationBasic,
  PlainUrlParameter, // not available in combination with Post method
  RequestBodyParameter
};

  }
}

